{
  "name": "enum-converter",
  "version": "0.0.0",
  "description": "Convert Enums from one language to another",
  "keywords": ["enum", "enums", "converter", "ast", "transpiler"],
  "author": "Nitzan.o <this.vptr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitzano/enum-converter.git"
  },
  "bugs": {
    "url": "https://github.com/nitzano/enum-converter/issues"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "bin": { "enumc": "./src/cli/cli.js" },
  "scripts": {
    "prebuild": "npm run clean:soft",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "build:prod": "npm run build -- -p tsconfig.prod.json",
    "postbuild": "cp package.json ./lib",
    "cli": "ts-node ./src/cli/cli.ts",
    "clean": "rimraf lib",
    "clean:soft": "rm -rf lib/*",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "run": "node dist/main.js",
    "test": "jest --verbose",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/.bin/jest -i",
    "test:watch": "jest --watch",
    "test:nodemon":
      "nodemon -L --watch __tests__ --watch src -e spec.ts --exec 'clear && npm run test'"
  },
  "dependencies": {
    "change-case": "3.0.2",
    "estree-walker": "0.5.1",
    "filbert": "0.1.20",
    "lodash": "4.17.5",
    "tmp": "0.0.33",
    "typescript": "2.8.1",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@types/estree": "0.0.38",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.106",
    "@types/node": "8.10.7",
    "@types/tmp": "0.0.33",
    "@types/yargs": "11.0.0",
    "jest": "22.4.3",
    "nodemon": "1.17.3",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.2",
    "ts-node": "3.3.0",
    "tslint-config-prettier": "1.12.0",
    "tslint-plugin-prettier": "1.3.0",
    "tslint": "5.9.1"
  }
}
